<?xml version="1.0" encoding="utf-8"?>
<manifest package="im.hua.artofandroid"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ipc.SecondActivity"
            android:label="@string/title_activity_second"
            android:process=":remote"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".chapter_234.MessengerActivity"
            android:label="@string/title_activity_messenger"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <service android:name=".chapter_234.MessengerService"
            android:process=":remote"/>
    </application>

</manifest>